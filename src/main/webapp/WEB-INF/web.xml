<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>FunFit</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ParticipantControllerServlet</display-name>
    <servlet-name>ParticipantControllerServlet</servlet-name>
    <servlet-class>com.example.FunFit.controller.ParticipantControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ParticipantControllerServlet</servlet-name>
    <url-pattern>/ParticipantControllerServlet</url-pattern>
    <url-pattern>/AddParticipant</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BatchControllerServlet</display-name>
    <servlet-name>BatchControllerServlet</servlet-name>
    <servlet-class>com.example.FunFit.controller.BatchControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BatchControllerServlet</servlet-name>
    <url-pattern>/BatchControllerServlet</url-pattern>
    <url-pattern>/AddBatch</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>updateparticipanttest</display-name>
    <servlet-name>updateparticipanttest</servlet-name>
    <servlet-class>com.example.FunFit.controller.updateparticipanttest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateparticipanttest</servlet-name>
    <url-pattern>/updateparticipanttest</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BatchUpdateServlet</display-name>
    <servlet-name>BatchUpdateServlet</servlet-name>
    <servlet-class>com.example.FunFit.controller.BatchUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BatchUpdateServlet</servlet-name>
    <url-pattern>/BatchUpdateServlet</url-pattern>
     <url-pattern>/UpdateBatch</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ParticipantUpdateServlet</display-name>
    <servlet-name>ParticipantUpdateServlet</servlet-name>
    <servlet-class>com.example.FunFit.controller.ParticipantUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ParticipantUpdateServlet</servlet-name>
    <url-pattern>/ParticipantUpdateServlet</url-pattern>
    <url-pattern>/UpdateParticipant</url-pattern>
  </servlet-mapping>
</web-app>